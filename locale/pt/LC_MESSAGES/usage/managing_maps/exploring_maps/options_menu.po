#: ../../usage/managing_maps/exploring_maps/options_menu.rst:35
#: bd585dd78bcc4be78a1b72a445b229c1
msgid "customize the *Layout*"
msgstr "personalizar o *Layout*"

#: ../../usage/managing_maps/exploring_maps/options_menu.rst:37
#: 032fe2067b67442c886f5731c01a969b
msgid "the *Sheet size* (A3, A4);"
msgstr "o *Tamanho da folha* (A3, A4);"

#: ../../usage/managing_maps/exploring_maps/options_menu.rst:38
#: 485ec4a9f66c42278bb410d2dd9251c0
