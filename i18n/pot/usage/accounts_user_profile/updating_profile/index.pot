msgstr ""

#: ../../usage/accounts_user_profile/updating_profile/index.rst:38
#: 1407658de7cb4d958f148ebe79b6e0f2
msgid "Click the :guilabel:`Delete from Favorites` button to remove the resource from the list."
msgstr ""

#: ../../usage/accounts_user_profile/updating_profile/index.rst:40
